*{margin:0;
padding:0;
}
.clear-float{
	clear:both;
}
html,body {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	margin: 0;
	height: 100%;
}
a{
	text-decoration:none;
	color: #000
}
ul,li{list-style-type:none; 
	list-style:none;
	font-size:11px;
} 
#leftnav ul{ display:table; height:90% }
#leftnav img { height:47px; display:block; }
#leftnav li { display:table-row; padding-right:2px; }
#leftnav li span { display:table-cell; vertical-align:middle;}
/*#leftnav li:first-child,
#leftnav li:last-child { height:20px }*/

/*No Selected tabs*/
.homeNs{
	background: #f7dbec;	
}
.menuNs{
	background: #ecdbf7;	
}
.openingNs{
	background: #dee5fc;	
}
.loginNs{
	background: #faf9dd;	
}
.activityNs{
	background: #e0fadd;	
}

/*hover tabs*/
.homeNs :hover{
	background: red;	
}
.menuNs :hover{
	background: #d9b8ef;	
}
.openingNs :hover{
	background: #beccf9;	
}
.loginNs :hover{
	background: #f4f4bb;	
}
.activityNs :hover{
	background: #c1f5bc;	
}
/*Selected tabs*/
