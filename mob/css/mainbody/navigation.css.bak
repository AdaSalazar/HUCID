#tile{}
#rightnav{float:right;
margin-right:2px;}
.nav{ background: rgba( #999,0.80);
	text-align:center;

	padding:3px 3px 5px 10px;
	text-decoration:none;
	list-style:none;
	list-style-type:none;

}
.nav ul{
		display:inline;
	background:rgba(204,204,204,0.20);
	-moz-border-radius:0.8em;
	text-decoration:none;
	-webkit-border-radius:0.8em;
	border-radius:0.8em;
	-moz-box-shadow:0px 0px 5px #dddddd;
	-webkit-box-shadow:0px 0px 5px #dddddd;
	box-shadow:0px 0px 5px #dddddd;
}
.nav li{
	list-style:none;
	list-style-type:none;
	height:30px;
	line-height:22px;
	text-align:left;
 
	text-transform:capitalize;
	display:block;
	border-bottom:1px solid #dddddd;
	-moz-border-radius:0.8em;
	-webkit-border-radius:0.8em;
	border-radius:0.8em;
	-moz-box-shadow:0px 0px 5px #dddddd;
	-webkit-box-shadow:0px 0px 5px #dddddd;
	box-shadow:0px 0px 5px #dddddd;
}
.nav li:last-child{
	border:none;
}
.nav a{
	line-height:30px;
	display:block;
	color:#5d5d5d;
	list-style:none;	padding-left:33px;	
}
.navpp{

	text-align: left;
}
.nav a:hover{ background:rgba(0,153,155,0.10);
}
